# Database Configuration
DB_HOST=localhost
DB_PORT=6543
DB_USERNAME=your_username
DB_PASSWORD=your_password
DB_NAME=nqkly_db

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-make-it-long-and-random

# Application Configuration
NODE_ENV=development
PORT=3000

# Kudi SMS API Configuration
# Base URL for Kudi SMS API (typically https://api.kudisms.com/api/v1)
KUDI_SMS_API_URL=https://api.kudisms.com/api/v1
# Your Kudi SMS API key from https://kudisms.com dashboard
KUDI_SMS_API_KEY=your-kudi-sms-api-key
# Sender ID that appears on SMS (max 11 characters)
KUDI_SMS_SENDER_ID=NQkly
